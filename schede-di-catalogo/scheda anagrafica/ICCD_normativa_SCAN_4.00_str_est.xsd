											<xs:attribute name="binding_thesId" type="xs:string" use="optional" fixed="VC_LIR"/>
											<xs:attribute name="binding_levelExpr" type="xs:string" use="optional" fixed="$1"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
